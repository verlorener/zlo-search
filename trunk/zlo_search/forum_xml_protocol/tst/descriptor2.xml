<?xml version="1.0"?>

<forum xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../src/main/resources/info/xonix/zlo/search/xmlfp/xsd/descriptor.xsd">

    <name>Форум ФРТК-МФТИ</name>
    <description>Tra-la-la</description>
    <url>http://zlo.rt.mipt.ru/</url>

    <type>tree</type>

    <xmlfpUrls>
        <lastMessageNumberUrl>http://zlo.rt.mipt.ru/xmlfp?lastMsgNumber</lastMessageNumberUrl>
        <messageUrl>http://zlo.rt.mipt.ru/xmlfp?msg=__message_id__</messageUrl>
        <messageListUrl maxCount="1000">http://zlo.rt.mipt.ru/xmlfp?from=__from__&amp;to=__to__</messageListUrl>
    </xmlfpUrls>

    <forumUrls>
        <messageUrl>http://forum/msg/__message_id__</messageUrl>
        <userProfileUrl>http://forum/user/__user_id__</userProfileUrl>
    </forumUrls>
</forum>